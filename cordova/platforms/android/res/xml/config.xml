<?xml version='1.0' encoding='utf-8'?>
<widget id="com.gabageapps.numberplace" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidLaunchMode" value="singleTop" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="AdMob">
        <param name="android-package" value="com.rjfun.cordova.plugin.AdMob" />
    </feature>
    <icon density="mdpi" src="./www/images/launch-icons/android/mdpi.png" />
    <icon density="hdpi" src="./www/images/launch-icons/android/hdpi.png" />
    <icon density="xhdpi" src="./www/images/launch-icons/android/xhdpi.png" />
    <icon density="xxhdpi" src="./www/images/launch-icons/android/xxhdpi.png" />
    <icon density="xxxhdpi" src="./www/images/launch-icons/android/xxxhdpi.png" />
    <name>NP</name>
    <content src="index.html" />
    <access origin="*" />
    <preference name="Fullscreen" value="true" />
    <preference name="Orientation" value="portrait" />
</widget>
